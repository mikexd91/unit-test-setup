pipeline {
	agent any

	stages {

		stage ("install") {
			steps {
				echo "Install";
			}
		}

		stage ("Tests") {
			steps {
				parallel (

					eslint: {
						echo "eslint";
					},

					karma: {
						echo "karma";
					}

				)
			}
		}

		stage ("checkstyle") {
			steps {
				echo "Checkstyle";
			}
		}



	}
}